<table>

<% @offerings.each do |offering| %>
    <% if current_user && current_user.id == offering.attendee.id && offering.status == "Booked" %>
    <tr>
        <td><%= offering.organizer.id %></td>
        <td><%= offering.attendee.id %></td>
    </tr>
    <% end %>
<% end %>
