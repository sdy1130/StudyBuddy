<p>
My Tutoring
</p>
<table>
<% @offerings.each do |offering| %>
    <% if current_user && current_user.id == offering.organized.id && offering.status == "Free" %>
    <tr>
    <td><%= offering.organizer.id %></td>
    <td><%= offering.attendee.id %></td>
    </tr>
    <% end %>
<% end %>
</table>

